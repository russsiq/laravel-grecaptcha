{
    "name": "russsiq/laravel-grecaptcha",
    "description": "Laravel 6.* validator extension with Google reCAPTCHA v3.",
	"homepage": "https://github.com/russsiq/laravel-grecaptcha",
    "type": "library",
	"license": "MIT",
	"keywords": ["grecaptcha", "laravel", "laravel5"],
    "authors": [
        {
            "name": "Рустам Гимранов",
            "email": "rustam_gimranov@mail.ru",
            "homepage": "https://github.com/russsiq",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/russsiq/laravel-grecaptcha"
        }
    ],
    "require": {
        "php": ">=7.2.0"
    },
    "autoload": {
        "psr-4": {
            "Russsiq\\GRecaptcha\\": "src/app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Russsiq\\GRecaptcha\\GRecaptchaServiceProvider"
            ],
            "aliases": {
                "GRecaptcha": "Russsiq\\GRecaptcha\\Support\\Facades\\GRecaptcha"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
